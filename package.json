{
  "name": "windaid-institute",
  "version": "1.0.0",
  "private": true,
  "description": "WindaidInstitute",
  "author": "David Ghazi",
  "keywords": [
    "gatsby"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-arrow-functions",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-runtime"
    ]
  },
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "heroku-posbuild": "gatsby build",
    "lint": "npx eslint src/**/*.{js,jsx}",
    "lint:fix": "npx eslint src/**/*.{js,jsx} --fix"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@react-icons/all-files": "^4.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "bootstrap": "^4.6.0",
    "clsx": "^1.1.1",
    "dotenv": "^8.2.0",
    "gatsby": "^3.0.1",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-image": "^1.2.0",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^3.2.0",
    "gatsby-plugin-styled-components": "^4.0.0",
    "gatsby-source-contentful": "^5.2.1",
    "gatsby-source-filesystem": "^3.2.0",
    "gatsby-transformer-sharp": "^3.2.0",
    "react": "^17.0.1",
    "react-bootstrap": "^1.5.2",
    "react-countup": "^4.3.3",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "react-multi-carousel": "^2.6.2",
    "react-player": "^2.9.0",
    "react-router-dom": "^5.2.0",
    "react-visibility-sensor": "^5.1.1",
    "styled-components": "^5.2.1",
    "webfontloader": "^1.6.28"
  },
  "main": "gatsby-browser.js",
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/preset-react": "^7.13.13",
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^1.7.0",
    "gatsby-plugin-root-import": "^2.0.6",
    "husky": "^4.2.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  },
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --config .prettierrc --write",
      "eslint --fix"
    ]
  }
}
